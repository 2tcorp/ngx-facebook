{
  "name": "ng2-facebook-sdk",
  "version": "2.0.0-beta.2",
  "description": "Facebook TypeScript SDK for Angular2",
  "main": "dist/es5/index.js",
  "typings": "dist/es5/index.d.ts",
  "module": "dist/esm/index.js",
  "scripts": {
    "prebuild": "rm -rf dist aot",
    "build": "npm run build:esm && npm run build:es5",
    "prebuild:es5": "rm -rf dist/es5",
    "build:es5": "ngc -p tsconfig-es5.json",
    "prebuild:esm": "rm -rf aot dist/esm",
    "build:esm": "ngc",
    "pretest": "rm -rf dist/es5 && tsc -p tsconfig-es5.json",
    "test": "karma start karma.conf.js --single-run",
    "pretest:watch": "npm run pretest",
    "test:watch": "concurrently \"tsc -w -p tsconfig-es5.json\" \"karma start karma.conf.js\"",
    "docs": "zm-docs-gen"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zyramedia/ng2-facebook-sdk.git"
  },
  "keywords": [
    "facebook",
    "angular2",
    "sdk"
  ],
  "author": "Zyra Media Inc.",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zyramedia/ng2-facebook-sdk/issues"
  },
  "homepage": "https://github.com/zyramedia/ng2-facebook-sdk",
  "devDependencies": {
    "@angular/common": "2.4.10",
    "@angular/compiler": "2.4.10",
    "@angular/compiler-cli": "2.4.10",
    "@angular/core": "2.4.10",
    "@angular/platform-browser": "2.4.10",
    "@angular/platform-browser-dynamic": "2.4.10",
    "@types/jasmine": "^2.5.47",
    "@types/node": "^7.0.12",
    "@zyramedia/docs-gen": "^1.1.3",
    "babel-polyfill": "^6.23.0",
    "concurrently": "^3.4.0",
    "core-js": "^2.4.1",
    "jasmine-core": "^2.5.2",
    "karma": "^1.5.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-cli": "^1.0.1",
    "karma-jasmine": "^1.1.0",
    "karma-jasmine-html-reporter": "^0.2.2",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sauce-launcher": "^1.1.0",
    "lodash": "^4.17.4",
    "rxjs": "5.0.1",
    "systemjs": "^0.20.11",
    "typescript": "^2.2.2",
    "zone.js": "0.7.2"
  }
}
